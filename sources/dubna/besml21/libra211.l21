      SUBROUTINE LFSCAN(TEXT)
C*
C****   1.4.83   A.П.CAПOЖHИKOB
C*
C*   CИHTAKCИЧECKИЙ PAЗБOP ДИPEKTИB
C*   ФAЙЛOBOЙ CИCTEMЫ  O.H.ЛOMИДЗE
C*      ( CM. П/П   LFSYST )
C*
      INTEGER TEXT(14),STRD(20)
      COMMON /LFDIAG/ LFD(3)
      DIMENSION NAMATR(8),NAMVAL(8)
      DIMENSION NAMJOB(5),MD(4,4),ICP(4,5)
      DATA NAMATR
     *  / 'JOB', 'MASTER', 'LIMIT', 'FILE',
     *    'DATE', 'UNIT', 'REGIM', 'NAMFR' /
      DATA NAMJOB
     *  / 'MAKEF', 'OPENF', 'CLOSEF', 'DELETF',
     *    'FRIEND' /
      DATA KLF/10/,KTWOP/58/,KCOMMA/44/,
     *     KEQ/61/,KSTAR/42/,KBLAN/32/
      DATA MD
     1  /  'HEBEPHЫЙ PAЗДEЛИTEЛЬ   ',
     2     'HEBEPHOE ИMЯ ПAPAMETPA ',
     3     'OШ. ЗHAЧEHИE ПAPAMETPA ',
     4     'HE ЗAДAH ПAPAMETP JOB  ' /
      DATA ICP
     1  /   2, 3, 4, 0,
     2      2, 4, 6, 7,
     3      6, 0, 0, 0,
     4      2, 4, 0, 0,
     5      2, 4, 7, 8  /
C
C---  BЫБOPKA ЗAГOЛOBKA YПP.KAPTЫ
C
      CALL IPUTS(KLF,TEXT,72)
      CALL SCANER(TEXT(2),STRD,2,ITYP,KOD,IPOZ)
      DO 1 I=1,20
   1  STRD(I)=6H
      DO 2 I=1,8
   2  NAMVAL(I)=0
      JOB=0
      MERR=1
      IPERR=IPOZ-3
      IF(KOD.EQ.KTWOP) GOTO 10
C
C---   Д И A Г H O C T И K A :
C
   3  IPERR=IPERR+6
      DO 4 I=1,3
   4  CALL IPUTS(KSTAR,STRD,IPERR+I-1)
      DO 5 I=1,24
      LET=IGETS(MD(1,MERR),I-1)
   5  CALL IPUTS(LET,STRD,IPERR+I+5)
      PRINT 1000,STRD
      RETURN
C
C---  BЫБOPKA ПAPЫ: <ATPИБYT>=<ЗHAЧEHИE>
C
  10  CALL SCANER(0,IDENT,0,ITYP,KOD,IPOZ)
      MERR=1
      IPERR=IPOZ-3
      IF(KOD.NE.KEQ) GOTO 3
      MERR=2
      J = JSEART(NAMATR,1,8,IDENT)
      IF(J.EQ.0) GOTO 3
      CALL SCANER(0,LFD,2,ITYP,KOD,IPOZ)
      NAMVAL(J)=LFD
      MERR=1
      IPERR=IPOZ-3
      IF(KOD.NE.KCOMMA.AND.KOD.NE.KLF.AND.KOD.NE.KBLAN) GOTO 3
      ITYP=ITYP+1
      IPERR=IPOZ-4
      MERR=3
      GOTO (3,11,12,3,13,3), ITYP
C
C---  ПPOBEPKA ДOПYCTИMOCTИ ЗHAЧEHИЯ
C
  12  IF(J.EQ.3.OR.J.EQ.6) NAMVAL(J)=IOCTAL(NAMVAL(J))
  11  NAMVAL(J)=NAMVAL(J).OR.0
      IF(NAMVAL(J).LT.0) GOTO 3
      GOTO (3,3,23,3,26,25,3,3), J
  23  IF(NAMVAL(J).EQ.0.OR.NAMVAL(J).GT.4095) GOTO 3
      GOTO 100
  25  IF(NAMVAL(J).NE.0.AND.NAMVAL(J).LT.16) NAMVAL(J)=NAMVAL(J)+32
      IF(NAMVAL(J).LT.24.OR.NAMVAL(J).GT.55) GOTO 3
      GOTO 100
  26  IF(NAMVAL(J).GT.0.AND.NAMVAL(J).LT.830401) GOTO 3
      GOTO 100
  13  GOTO ( 31,100,3,100,3,3,100,100), J
  31  JOB = JSEART(NAMJOB,1,5,NAMVAL(J))
      IF(JOB.EQ.0) GOTO 3
C
C---  ПPOБA HA KOHEЦ CПИCKA ПAP
C
  100 IF(KOD.EQ.KCOMMA) GOTO 10
      MERR=4
      IPERR=4
      MD(4,MERR)=NAMATR
      IF(JOB.EQ.0) GOTO 3
C
C---   ПPOБЫ HA ПOЛHOTY KOMПЛEKTA ПAPAMETPOB
C      И PEAЛИЗAЦИЯ ДИPEKTИBЫ.
C
      DO 98 I=1,4
      K=ICP(I,JOB)
      IF(K.EQ.0) GOTO 99
      MD(4,MERR)=NAMATR(K)
      IF(NAMVAL(K).EQ.0) GOTO 3
  98  CONTINUE
  99  GOTO (101,102,103,104,105), JOB
C---
 101  K = LFCREF(NAMVAL(2),NAMVAL(3),NAMVAL(4),NAMVAL(5)) $ GOTO 111
C---
 102  K = LFOPEN(NAMVAL(2),NAMVAL(4),NAMVAL(6),NAMVAL(7)) $ GOTO 111
C---
 103  K = LFCLOS(NAMVAL(6)) $ GOTO 111
C---
 104  K = LFDELF(NAMVAL(2),NAMVAL(4)) $ GOTO 111
C---
 105  K = LFRIEN(NAMVAL(2),NAMVAL(4),NAMVAL(8),NAMVAL(7))
C---
 111  IF(K.NE.0) PRINT 1001,LFD
      RETURN
C---
 1000 FORMAT(1X,20A6)
 1001 FORMAT('  ***  ',3A6)
C---
      END
