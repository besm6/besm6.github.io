      SUBROUTINE PTCHEK(JOB)
C*
C***     KOHTPOЛЬ ПEPФOЛEHTЫ HA ЧETHOCTЬ
C*
C*          157/  - C ПEЧATЬЮ
C*          257/  - БEЗ OHOЙ
C*
      DIMENSION MTIT(4),M1(21),M2(21),MERS(3)
      DATA MERS/'OШ.ЧETHOCTЬ:212'/
      DATA MTIT/' KOHTPOЛЬ ПEPФOЛEHTЫ.'/
      DIMENSION IDK(3)
      DATA IDK/' HET   HEЧET ЧET'/
      DATA NER/0/
C---
      J=JOPER(MTIT,4,0)
      J = JOPER('TИП= 1-HEЧET, 2-ЧET, 0-BCE PABHO',6,1)
      MP=1 $ IF(J.EQ.'1') GOTO 1
      MP=2 $ IF(J.EQ.'2') GOTO 1
      MP=0 $ IF(JOB.NE.1) GOTO 999
C
C   MP - TИП KOHTPOЛЯ
C   KEC - CИMBOЛ KOHЦA KAДPA
C   LAST - ПOЗИЦИЯ ПPEДЫД.KOHЦA KAДPA
C
  1   KEC=0 $ IF(JOB.NE.1) GOTO 3
      J=JOPER('KOД KOHЦA KAДPA =',3,1)
      DO 2 I=1,3
      LET=IGETS(J,I-1)
      IF(LET.EQ.32) GOTO 3
  2   KEC=8*KEC+LET-48
  3   KEC=KEC.AND.177B.OR.0
      IF(JOB.EQ.1) PRINT 1002,IDK(MP+1),KEC
C---
  4   LET=INPSPT(8) $ IF(LET.EQ.0) GOTO 4
C---
  5   NS=0 $ MER=0 $ LAST=0
      DO 6 I=1,21
      M1(I)=6H
  6   M2(I)=6H
  7   LET1=LET.AND.177B.OR.0
      L=LET1 $ IF(L.EQ.KEC) L=62
      K=1 $ IF(LET1.LT.32) K=5
      IF(NS+K.LT.124) GOTO 8
      IF(JOB.NE.1) GOTO 5
      PRINT 1000,M1
      IF(MER.NE.0) PRINT 1000,M2
      GOTO 5
C---
  8   MER1=0 $ IF(MP.EQ.0) GOTO 10
      N1 = LCNT( LET.AND.377B ) .AND.1
      IF(N1.EQ.1.AND.MP.EQ.1) GOTO 10
      IF(N1.EQ.0.AND.MP.EQ.2) GOTO 10
      MER1=1 $ ENCODE(6,1001,MERS(3)) LET
      MER=MER+1 $ CALL IPUTS(77B,M2,NS+K/5)
      NER=NER+1 $ J=JOPER(MERS,3,1)
C---
  10  IF(K.EQ.1) GOTO 19
      IF(MER1.EQ.0.AND.L.EQ.62) GOTO 19
      CALL IPUTS(42B,M1,NS) $ NS=NS+1
      L=LET.SHIFT.6.AND.3.OR.48
      IF(L.NE.48) GOTO 14
      L=LET.SHIFT.3.AND.7.OR.48
      IF(L.EQ.48) GOTO 16
      GOTO 15
  14  CALL IPUTS(L,M1,NS) $ NS=NS+1
      L=LET.SHIFT.3.AND.7.OR.48
  15  CALL IPUTS(L,M1,NS) $ NS=NS+1
  16  L=LET.AND.7.OR.48
      CALL IPUTS(L,M1,NS) $ NS=NS+1
      L=42B
  19  CALL IPUTS(L,M1,NS) $ NS=NS+1
      ASSIGN 7 TO NEXT
      IF(LET1.NE.KEC) GOTO 30
      LCDR=NS-LAST $ LAST=NS
      IF(NS+LCDR.LT.124) GOTO 30
      ASSIGN 5 TO NEXT $ IF(JOB.NE.1) GOTO 30
      PRINT 1000,M1
      IF(MER.NE.0) PRINT 1000,M2
  30  LET = INPSPT(8)
      IF(LET.NE.0) GOTO NEXT,(5,7)
      IF(JOB.NE.1) MER=0
      IF(JOB.EQ.1) PRINT 1000,M1
      IF(MER.NE.0) PRINT 1000,M2
      ENCODE(18,1003,MERS) NER
      J=JOPER(MERS,3,0)
C---
 999  J=JOPER('ДO CBИДAHИЯ.',2,0)
      STOP
 1000 FORMAT(1X,21A6)
 1001 FORMAT(O3,3X)
 1002 FORMAT('  KOHTPOЛЬ П/Л : ',A6,'    KOHEЦ KAДPA : ',O3)
 1003 FORMAT('ЧИCЛO OШИБOK:',I5)
      END
