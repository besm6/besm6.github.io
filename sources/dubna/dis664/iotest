>,ALUS=DА,RА=WR6,FUNC=OR,Н,ALUD=В,RВ=WR6
CONT ALUS=DZ,Н,ALUD=ВА,RА=WR6,RВ=WR6,ALU,DSRC=SHIFT,MPS,PSHF=SН0+8,SHF=LOG
CONT YDEV=МРМЕМ,IОМР,MPADR=10,DSRC=SHIFT,MPS,PSHF=SН0-56,SHF=LOG
>,ALUS=DА,RА=WR6,FUNC=OR,Н,ALUD=В,RВ=WR6
CONT ALUS=DZ,Н,ALUD=ВА,RА=WR6,RВ=WR6,ALU,DSRC=SHIFT,MPS,PSHF=SН0+8,SHF=LOG
CONT YDEV=МРМЕМ,IОМР,MPADR=9,DSRC=SHIFT,MPS,PSHF=SН0-56,SHF=LOG
>,ALUS=DА,RА=WR6,FUNC=OR,Н,ALUD=В,RВ=WR6
CONT ALUS=DZ,Н,ALUD=ВА,RА=WR6,RВ=WR6,ALU,DSRC=SHIFT,MPS,PSHF=SН0+8,SHF=LOG
CRTN YDEV=МРМЕМ,IОМР,MPADR=8,DSRC=SHIFT,MPS,PSHF=SН0-56,SHF=LOG
>,ALUS=DА,RА=WR6,FUNC=OR,Н,ALUD=В,RВ=WR6
;
RETN:ENTRY
;===== ВОЗВРАТ
CRTN
;
SТОР:ENTRY
;===== ОБЩИЙ ОСТАНОВ ДЛЯ ВСЕХ П/П
