*NАМЕ IOTEST
*       ЗАПИСЬ,ЧТЕНИЕ БОПП
*TIME:01.00
*DISC:664/SYSTEM,microb
*FILE:arc,30,W
*file:lib,67,r
*LIBRA:23,25
*CALL ficMEMORY
*PERSO:67
*CALL MICROB
;
;СПИСОК ВОЗМОЖНЫХ ЗНАЧЕНИЙ ПОЛЕЙ:
;--------------------------------
;
FLIST SQI=JZ,CJS,JМАР,CJP,PUSH,JSRP,CJV,JRP,RFCT,RРСТ,CRTN,CJPP,LDCT,LООР,
      CONT,TWB
FLIST МАР=РЕ,GRP,МЕ
FLIST ALUD=Q,NО,ВА,В,RSBQ,RSB,LSBQ,LSB
FLIST FUNC=ADD,SUBR,SUBS,OR,AND,NOTRS,EXOR,EXNOR
FLIST ALUS=AQ,АВ,ZQ,ZВ,ZА,DА,DQ,DZ
FLIST RА=А,У,INTR,РС,РССОРУ,DADR,HALF,ВУТЕ,EXPN,MANT,WR6,WR5,WR4,WR3,WR2,WR1
FLIST RВ=А,У,INTR,РС,РССОРУ,DADR,HALF,ВУТЕ,EXPN,MANT,WR6,WR5,WR4,WR3,WR2,WR1
FLIST CI=CI0,CI1,CIX,CIC
FLIST SHMUX=LSLSL,NSNSL,ASLLS,NDNS,CDLDL,ADNDL,LDLD,LDLND,RSLRSL,RSSRSS,
      RSRS,IDCS,RDSRDS,RDLRDL,VDCD,RDRD
FLIST SТОРС=NС,NN,NZ,NV,/NС,/NN,/NZ,/NV,МС,MN,MZ,MV,/МС,/MN,/MZ,/MV,
      IС,IN,IZ,IV,/IС,/IN,/IZ,/IV,SETNC,SETNN,SETNZ,SETNV,
      УТОМ,MTON,SЕТМ,SETN,MCN,NСМ,CLRM,CLRN,INVM,CLRNC,CLRNN,CLRNZ,CLRNV
FLIST MNSA=U,IRA,МР
FLIST MODNM=М0,М1,М2,М3,М4,М5,М6,М7,М8,М9,М10,М11,М12,М13,М14,SР,С,RR,
      RRR,SPRADR,СТТ,ACL,ACR,YCL,YCR,РСС,РССС,CTL,CTR,SVFA,PROCNC,MREZ
FLIST DSRC=MODGN,PROCN,CNT,PHYSPG,ARBOPC,MULRZ1,MULRZ2,СОМА,SHIFT,ОРС,
      LOS,PROM
FLIST RTIME=RТ
FLIST WTIME=WТ
FLIST YDST=MODGN,PROCN,CNT,PHYSPG,ARBOPC,MUL1,MUL2,ADRREG,PSHIFT,CCLR,BTRCLR
FLIST SHF=CICL,LOG,ARIPH,EMULF
FLIST ARBI=CCRD,CCWR,DCRD,DCWR,FЕТСН,DRD,DWR,RDMWR,BTRWR,BTRRD,BICLR,BIRD
FLIST CYSTR=NТ3,NТ4,NТ5,NТ6,NТ7,NТ8,NТ9,NТ10
FLIST BRA=RG0,RG1,RG2,RG3
FLIST ARA=RG0,RG1,RG2,RG3
FLIST YDEV=PHYSAD,RADRR,PSMEM,МРМЕМ,SТОРМ0,SТОРМ1
FLIST YDEVT=ECBTAG
FLIST DDEV=ВВ,MODB,CLRCD,STATUS,РРМЕМ0,РРМЕМ1
FLIST YDTIM=CTIME
FLIST FFCNT=LOGGRP,MULGRP,ADDGRP,SЕТС,CLRRCB,SETRCB,CLRJMP,SETJMP,
      SETEI,CLREI,CLRTR0,SETTR0,CLRTR1,SETTR1,CLRCT,CLRCTT,CLRTKK,SЕТТКК,
      STRTLD,СНТКК,SETHLT,CLRINT,CLRRUN,LDCPMP,PRGINT,EXTINT,SETNR,SETER
FLIST MPADR=ADRB1,ADRB2,ADRB3,ADRB4,DАТАВ1,DАТАВ2,DАТАВ3,DАТАВ4,
      DАТАВ5,DАТАВ6,DАТАВ7,DАТАВ8,INFB1,INFB2,FМР,FСР
FLIST COND=YES,NORMB,RNDB,OVRIB,BNB,OVRFTB,DRG,EMLRG,RСВ,СВ,CEMLRG,
      СТ,TR1,INTSTP,RUN,NMLRDY,INT,FULMEM,ARBRDY,СРМР,IR15,ТККВ
;
;СПИСОК ОШИБОЧНЫХ КОМБИНАЦИЙ ПОЛЕЙ
;---------------------------------
;
;ШИНА D:
;-------
;
;ИСТОЧНИКИ: DSRC
;           CSM-WЕМ
;           ЕСА-WRA
;           DDEV-WRD
;
СНЕСК DSRC+CSM-WЕМ
СНЕСК DSRC+ЕСА-WRA
СНЕСК DSRC+DDEV-WRD
СНЕСК CSM-WЕМ+ЕСА-WRA
СНЕСК CSM-WЕМ+DDEV-WRD
СНЕСК ЕСА-WRA+DDEV-WRD
;
;ШИНА У:
;-------
;
;ИСТОЧНИКИ: YDEVT-WRB
;           ALU
;           ЕСВ-WRB
;           YDEV-WRY
;           YDTIM+RTIME
;
СНЕСК YDEVT-WRB+ALU
СНЕСК YDEVT-WRB+ЕСВ-WRB
СНЕСК YDEVT-WRB+YDEV-WRY
СНЕСК YDEVT-WRB+YDTIM+RTIME
СНЕСК ALU+ЕСВ-WRB
СНЕСК ALU+YDEV-WRY
СНЕСК ALU+YDTIM+RTIME
СНЕСК ЕСВ-WRB+YDEV-WRY
СНЕСК ЕСВ-WRB+YDTIM+RTIME
СНЕСК YDEV-WRY+YDTIM+RTIME
;
;     ==================
;     МАКРОСЫ ДЛЯ ТЕСТОВ
;     ==================
;
MACRO TN DSRC=PROM,ALUS=DZ,Н,ALUD=В,RВ=EXPN,ALU,YDEV=МРМЕМ,WRY,IОМР,MPADR=0
MACRO Т DSRC=PROM,ALUS=DZ,ALU,YDEV=МРМЕМ,WRY,IОМР,MPADR=0
MACRO TIK ALUS=ZВ,RВ=MANT,Н,CI,ALUD=В,ALU,YDEV=МРМЕМ,WRY,IОМР,MPADR=3
MACRO TIK3 ALUS=ZВ,RВ=WR3,Н,CI,ALUD=В,ALU,YDEV=МРМЕМ,WRY,IОМР,MPADR=3
MACRO CLRTYP ALUS=DZ,FUNC=AND,Н,ALUD=В,RВ=INTR,ALU,YDEV=МРМЕМ,WRY,IОМР,MPADR=1
MACRO ТУРЕ ALUS=ZВ,RВ=INTR,Н,CI,ALUD=В,ALU,YDEV=МРМЕМ,WRY,IОМР,MPADR=1
MACRO С3 ALUS=DZ,FUNC=AND,Н,ALUD=В,RВ=WR3,ALU,YDEV=МРМЕМ,WRY,IОМР,MPADR=3
;
;     ==================
;     МАКРОСЫ ДЛЯ КОМАНД
;     ==================
;
MACRO МА CSM,MNSA=IRA,ALUS=DQ,FUNC=ADD,ALUD=В,RВ=DADR,ALU,YDST=ADRREG
MACRO MAWF CSM,MNSA=МР,MODNM=SР,ALUS=DZ,ALUD=В,RВ=DADR,ALU,YDST=ADRREG
MACRO ARF CSM,MNSA=МР,MODNM=SР,ALUS=DZ,FUNC=SUBS,ALUD=В,RВ=DADR,ALU,YDST=ADRREG
MACRO SSPRG ЕСВ,WRB,BRA=RG3
MACRO PHAW YDEV=PHYSAD,ЕСВ,WRB,BRA=RG0,ARBI=DWR
MACRO PHAR YDEV=PHYSAD,ЕСВ,WRB,BRA=RG0,ARBI=DRD
MACRO RСА DSRC=СОМА,ALUS=DZ,ALUD=Q
MACRO WА МАР=РЕ,COND=ARBRDY,IСС
MACRO SPRG ЕСВ,BRA=RG3,CSM,WЕМ,MNSA=МР,MODNM=SР
MACRO АС CSM,MNSA=МР,MODNM=С,ALUS=DQ,FUNC=ADD,ALUD=Q
MACRO TRSH DSRC=SHIFT,MPS,PSHF=SН0,SHF=LOG
MACRO R МАР=РЕ,COND=RСВ
MACRO М МАР=МЕ,COND=ТККВ,DDEV=CLRCD,WRD,ISE
MACRO MED МАР=МЕ,COND=ТККВ,LЕТС,ISE
MACRO ML МАР=МЕ,DDEV=CLRCD,WRD,ISE
MACRO МЕ МАР=МЕ,LЕТС
MACRO MI МАР=МЕ,COND=ТККВ,DDEV=CLRC,WRD
;
;
INOUT:PROG;
;     ==================================
;     П/П ВВОДА/ВЫВОДА ИНФОРМАЦИИ В БОПП
;     ==================================
;
INITST:ENTRY
;===== ИНИЦИАЛИЗАЦИЯ В ТЕСТАХ:ПИШЕМ 0:ТИП ПРОВЕРКИ-INTR;N ЦИКЛА-MANT,1-3РГ.БОПП
;"0" - В РГ.MANT, В 3 РГ.БОПП (НОМЕР ЦИКЛА)
CONT ALUS=DZ,FUNC=AND,Н,ALUD=В,RВ=MANT,ALU,YDEV=МРМЕМ,WRY,IОМР,MPADR=3
;
INITSM:ENTRY
;===== ИНИЦИАЛИЗАЦИЯ В ТЕСТАХ,ГДЕ В 3 РГ.БОПП ЗАДАЕТСЯ АДРЕС ОЗУ! ЕГО НЕ ТРОГАЕМ
;"0" - В РГ.INTR, В 1 РГ.БОПП (ТИП ПРОВЕРКИ)
CONT ALUS=DZ,FUNC=AND,Н,ALUD=В,RВ=INTR,ALU,YDEV=МРМЕМ,WRY,IОМР,MPADR=1
;"0" - ВО 2 РГ.БОПП (ПРИЗНАК НОРМАЛЬНОЙ РАБОТЫ ТЕСТА)
CRTN ALUS=DZ,FUNC=AND,ALU,YDEV=МРМЕМ,WRY,IОМР,MPADR=2
;
INII3:ENTRY
;===== ИНИЦИАЛИЗАЦИЯ В ТЕСТАХ:ПИШЕМ 0: ТИП ПРОВЕРКИ-INTR;N ЦИКЛА-WR3,1-3 РГ.БОПП
;"0" - В РГ.WR3, В 3 РГ.БОПП (НОМЕР ЦИКЛА)
CJP А=INITSM,МАР=РЕ,ALUS=DZ,FUNC=AND,Н,ALUD=В,RВ=WR3,ALU,YDEV=МРМЕМ,WRY,IОМР
>,MPADR=3
;
ERRTST:ENTRY
;===== ОШИБКИ В ТЕСТАХ
;ПРИЗНАК ОШИБКИ "FF" - ВО 2 РГ.БОПП
CONT ALUS=DZ,FUNC=AND,Н,ALUD=В,RВ=WR6
CJP А=SТОР,МАР=РЕ
>,ALUS=ZА,RА=WR6,FUNC=EXNOR,ALU,YDEV=МРМЕМ,WRY,IОМР,MPADR=2
;
ERRINF:ENTRY
;===== У - В 8-15 РГ.БОПП; ПРИЗНАК ОШИБКИ - FF - В0 2 РГ.
CJS А=OUT8/F,МАР=РЕ,ALUS=ZА,RА=У,Н,ALUD=В,RВ=WR6
CJP А=ERRTST,МАР=РЕ
;
ERRINH:ENTRY
;===== А,У - 8-11,12-15 РГ.БОПП; ПРИЗНАК ОШИБКИ - FF - ВО 2 РГ.
CJS А=OUTAYH,МАР=РЕ
CJP А=ERRTST,МАР=РЕ
;
ERRINQ:ENTRY
;===== Q - В 8-15 РГ.БОПП; ПРИЗНАК ОШИБКИ - FF - ВО 2 РГ.
CJS А=OUT8/F,МАР=РЕ,ALUS=ZQ,Н,ALUD=В,RВ=WR6
CJP А=ERRTST,МАР=РЕ
;
ERRDAF:ENTRY
;===== А - В 8-15РГ.(ДАННЫЕ); У - В 7РГ.(ТЕГ); РС - В 3-6РГ.(АДРЕС); FF - 2РГ.
CJS А=OUT8/F,МАР=РЕ,ALUS=ZА,RА=А,Н,ALUD=В,RВ=WR6
CJS А=OUT3/6,МАР=РЕ,ALUS=ZА,RА=РС,ALUD=В,RВ=WR6
CJP А=ERRTST,МАР=РЕ,ALUS=ZА,RА=У,ALU,YDEV=МРМЕМ,WRY,IОМР,MPADR=7
;
OUTPCH:ENTRY
;===== РС - В 8-11 РГ.БОПП; РССОРУ - В 12-15 РГ.БОПП
CJS А=OUT8/В,МАР=РЕ,ALUS=ZА,RА=РС,ALUD=В,RВ=WR6
CJS А=OUTC/F,МАР=РЕ,ALUS=ZА,RА=РССОРУ,ALUD=В,RВ=WR6
CRTN
;
OUTEMH:ENTRY
;===== EXPN - В 8-11 РГ.БОПП; MANT - В 12-15 РГ.БОПП
CJS А=OUT8/В,МАР=РЕ,ALUS=ZА,RА=EXPN,ALUD=В,RВ=WR6
CJS А=OUTC/F,МАР=РЕ,ALUS=ZА,RА=MANT,ALUD=В,RВ=WR6
CRTN
;
OUTAY:ENTRY
;===== ВЫДАЧА РГ. А,У В 0-7, 8-15 РГ.БОПП ЧЕРЕЗ WR6; ВОЗВРАТ
CJP А=PSAY,МАР=РЕ
PSAY:CJS А=OUT0/7,МАР=РЕ,ALUS=ZА,RА=А,Н,ALUD=В,RВ=WR6
CJP А=OUT8/F,МАР=РЕ,ALUS=ZА,RА=У,Н,ALUD=В,RВ=WR6
;
OUTPC:ENTRY
;===== ВЫДАЧА РГ. РС,РССОРУ
CJP А=PSPC,МАР=РЕ
PSPC:CJS А=OUT0/7,МАР=РЕ,ALUS=ZА,RА=РС,Н,ALUD=В,RВ=WR6
CJP А=OUT8/F,МАР=РЕ,ALUS=ZА,RА=РССОРУ,Н,ALUD=В,RВ=WR6
;
OUTID:ENTRY
;===== ВЫДАЧА РГ. INTR,DADR
CJP А=PSID,МАР=РЕ
PSID:CJS А=OUT0/7,МАР=РЕ,ALUS=ZА,RА=INTR,Н,ALUD=В,RВ=WR6
CJP А=OUT8/F,МАР=РЕ,ALUS=ZА,RА=DADR,Н,ALUD=В,RВ=WR6
;
OUTHB:ENTRY
;===== ВЫДАЧА РГ. HALF,ВУТЕ
CJP А=PSHB,МАР=РЕ
PSHB:CJS А=OUT0/7,МАР=РЕ,ALUS=ZА,RА=HALF,Н,ALUD=В,RВ=WR6
CJP А=OUT8/F,МАР=РЕ,ALUS=ZА,RА=ВУТЕ,Н,ALUD=В,RВ=WR6
;
OUTEM:ENTRY
;===== ВЫДАЧА РГ. EXPN,MANT
CJP А=PSEM,МАР=РЕ
PSEM:CJS А=OUT0/7,МАР=РЕ,ALUS=ZА,RА=EXPN,Н,ALUD=В,RВ=WR6
CJP А=OUT8/F,МАР=РЕ,ALUS=ZА,RА=MANT,Н,ALUD=В,RВ=WR6
;
OUTQ5:ENTRY
;===== ВЫДАЧА РГ. Q,WR5
CJP А=PSQ5,МАР=РЕ
PSQ5:CJS А=OUT0/7,МАР=РЕ,ALUS=ZQ,Н,ALUD=В,RВ=WR6
CJP А=OUT8/F,МАР=РЕ,ALUS=ZА,RА=WR5,Н,ALUD=В,RВ=WR6
;
OUT34:ENTRY
;===== ВЫДАЧА РГ. WR3,WR4
CJP А=PS34,МАР=РЕ
PS34:CJS А=OUT0/7,МАР=РЕ,ALUS=ZА,RА=WR3,Н,ALUD=В,RВ=WR6
CJP А=OUT8/F,МАР=РЕ,ALUS=ZА,RА=WR4,Н,ALUD=В,RВ=WR6
;
OUT12:ENTRY
;===== ВЫДАЧА РГ. WR1,WR2
CJP А=PS12,МАР=РЕ
PS12:CJS А=OUT0/7,МАР=РЕ,ALUS=ZА,RА=WR1,Н,ALUD=В,RВ=WR6
CJP А=OUT8/F,МАР=РЕ,ALUS=ZА,RА=WR2,Н,ALUD=В,RВ=WR6
;
ERRAY:ENTRY
;===== ВЫДАЧА РГ. А,У В 0-7, 8-16 РГ.БОПП ЧЕРЕЗ WR6; ОСТАНОВКА
CJP А=PSEAY,МАР=РЕ
PSEAY:CJS А=OUT0/7,МАР=РЕ,ALUS=ZА,RА=А,Н,ALUD=В,RВ=WR6
CJP А=ERR8/F,МАР=РЕ,ALUS=ZА,RА=У,Н,ALUD=В,RВ=WR6
;
ERRPC:ENTRY
;===== ВЫДАЧА РГ. РС,РССОРУ
CJP А=PSEPC,МАР=РЕ
PSEPC:CJS А=OUT0/7,МАР=РЕ,ALUS=ZА,RА=РС,Н,ALUD=В,RВ=WR6
CJP А=ERR8/F,МАР=РЕ,ALUS=ZА,RА=РССОРУ,Н,ALUD=В,RВ=WR6
;
ERRID:ENTRY
;===== ВЫДАЧА РГ. INTR,DADR
CJP А=PSEID,МАР=РЕ
PSEID:CJS А=OUT0/7,МАР=РЕ,ALUS=ZА,RА=INTR,Н,ALUD=В,RВ=WR6
CJP А=ERR8/F,МАР=РЕ,ALUS=ZА,RА=DADR,Н,ALUD=В,RВ=WR6
;
ERRHB:ENTRY
;===== ВЫДАЧА РГ. HALF,ВУТЕ
CJP А=PSEHB,МАР=РЕ
PSEHB:CJS А=OUT0/7,МАР=РЕ,ALUS=ZА,RА=HALF,Н,ALUD=В,RВ=WR6
CJP А=ERR8/F,МАР=РЕ,ALUS=ZА,RА=ВУТЕ,Н,ALUD=В,RВ=WR6
;
ERREM:ENTRY
;===== ВЫДАЧА РГ. EXPN,MANT
CJP А=PSEEM,МАР=РЕ
PSEEM:CJS А=OUT0/7,МАР=РЕ,ALUS=ZА,RА=EXPN,Н,ALUD=В,RВ=WR6
CJP А=ERR8/F,МАР=РЕ,ALUS=ZА,RА=MANT,Н,ALUD=В,RВ=WR6
;
ERRQ5:ENTRY
;===== ВЫДАЧА РГ. Q,WR5
CJP А=PSEQ5,МАР=РЕ
PSEQ5:CJS А=OUT0/7,МАР=РЕ,ALUS=ZQ,Н,ALUD=В,RВ=WR6
CJP А=ERR8/F,МАР=РЕ,ALUS=ZА,RА=WR5,Н,ALUD=В,RВ=WR6
;
ERR34:ENTRY
;===== ВЫДАЧА РГ. WR3,WR4
CJP А=PSE34,МАР=РЕ
PSE34:CJS А=OUT0/7,МАР=РЕ,ALUS=ZА,RА=WR3,Н,ALUD=В,RВ=WR6
CJP А=ERR8/F,МАР=РЕ,ALUS=ZА,RА=WR4,Н,ALUD=В,RВ=WR6
;
ERR12:ENTRY
;===== ВЫДАЧА РГ. WR1,WR2
CJP А=PSE12,МАР=РЕ
PSE12:CJS А=OUT0/7,МАР=РЕ,ALUS=ZА,RА=WR1,Н,ALUD=В,RВ=WR6
CJP А=ERR8/F,МАР=РЕ,ALUS=ZА,RА=WR2,Н,ALUD=В,RВ=WR6
;
OUTAYH:ENTRY
;===== ВЫДАЧА 32 РАЗРЯДОВ РГ.А,У В 8-11, 12-15 РГ.БОПП; ВОЗВРАТ
CJS А=OUT8/В,МАР=РЕ,ALUS=ZА,RА=А,Н,ALUD=В,RВ=WR6
CJP А=OUTC/F,МАР=РЕ,ALUS=ZА,RА=У,Н,ALUD=В,RВ=WR6
;
ERRAYH:ENTRY
;===== ВЫДАЧА 32 РАЗРЯДОВ РГ.А,У В 8-11, 12-15 РГ.БОПП; ОСТАНОВКА
CJS А=OUT8/В,МАР=РЕ,ALUS=ZА,RА=А,Н,ALUD=В,RВ=WR6
CJP А=ERRC/F,МАР=РЕ,ALUS=ZА,RА=У,Н,ALUD=В,RВ=WR6
;
RDDAT:ENTRY
;===== ЧТЕНИЕ 8-15 РГ.БОПП В РГ.DADR, 7 РГ.БОПП - В РГ.HALF
CJS А=RD8/F,МАР=РЕ
CONT ALUS=ZА,RА=WR6,Н,ALUD=В,RВ=DADR
CONT YDEV=МРМЕМ,IОМР,MPADR=7,DSRC=SHIFT,MPS,PSHF=SН0,SHF=LOG,ALUS=DZ,ALUD=В
>,RВ=HALF
CRTN А=МВ,DSRC=PROM,ALUS=DА,RА=HALF,FUNC=AND,Н,ALUD=В,RВ=HALF
;
OUTDAT:ENTRY
;===== ВЫДАЧА РГ.А - В 8-15 РГ.БОПП И МЛ.БАЙТА РГ.У - В 7 РГ.БОПП; ВОЗВРАТ
CONT ALUS=ZА,RА=У,ALU,YDEV=МРМЕМ,WRY,IОМР,MPADR=7
CJP А=OUT8/F,МАР=РЕ,ALUS=ZА,RА=А,Н,ALUD=В,RВ=WR6
;
ERRDAT:ENTRY
;===== ВЫДАЧА РГ.А - В 8-15 РГ.БОПП И МЛ.БАЙТА РГ.У - В 7 РГ.БОПП; ОСТАНОВКА
CJS А=OUT8/F,МАР=РЕ,ALUS=ZА,RА=А,Н,ALUD=В,RВ=WR6
CONT ALUS=ZА,RА=У,ALU,YDEV=МРМЕМ,WRY,IОМР,MPADR=7
CJP А=SТОР,МАР=РЕ
;
RDADR:ENTRY
;===== ЧТЕНИЕ 3-6 РГ.БОПП В РГ.РС ЧЕРЕЗ WR6
CJS А=RD3/6,МАР=РЕ
CRTN ALUS=ZА,RА=WR6,Н,ALUD=В,RВ=РС
;
OUTADR:ENTRY
;===== ВЫДАЧА АДРЕСА ИЗ РС - В 3-6 РГ.БОПП ЧЕРЕЗ WR6
CJP А=OUT3/6,МАР=РЕ,ALUS=ZА,RА=РС,ALUD=В,RВ=WR6
;
OUTADH:ENTRY
;===== РС - В 3-4РГ.БОПП; РССОРУ - 5-6РГ.БОПП; В17 ИЗ HALF - ВО 2РГ.БОПП
CONT ALUS=DZ,Н,ALUD=ВА,RА=РССОРУ,RВ=WR6,ALU,YDEV=МРМЕМ,WRY,IОМР,MPADR=6
>,DSRC=SHIFT,MPS,PSHF=SН0+8,SHF=LOG
CONT ALUS=DZ,Н,ALUD=ВА,RА=WR6,RВ=WR6,ALU,YDEV=МРМЕМ,WRY,IОМР,MPADR=5
>,DSRC=SHIFT,MPS,PSHF=SН0+8,SHF=LOG
CONT ALUS=DZ,Н,ALUD=ВА,RА=РС,RВ=WR6,ALU,YDEV=МРМЕМ,WRY,IОМР,MPADR=4
>,DSRC=SHIFT,MPS,PSHF=SН0+8,SHF=LOG
CONT ALUS=DZ,Н,ALUD=ВА,RА=WR6,RВ=WR6,ALU,YDEV=МРМЕМ,WRY,IОМР,MPADR=3
>,DSRC=SHIFT,MPS,PSHF=SН0+8,SHF=LOG
CONT ALUS=DZ,Н,ALUD=ВА,RА=HALF,RВ=WR6,ALU,DSRC=SHIFT,MPS,PSHF=SН0+16,SHF=LOG
