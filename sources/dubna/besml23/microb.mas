C*     <iMq pOlq>:VALUE <HOBOE zHA~EHiE>
C*
C*   8. KOMMEHTAPij.  BCE, ~TO ClEdYET zA
C*    CiMBOlOM ";" - qBlqETCq KOMMEHTAPiEM.
C*    B ~ACTHOCTi, ECli ";" CTOiT B 1 pOzicii,
C*    TO BCq CTPOKA - KOMMEHTAPij.
C*
C*   9. YpPABlEHiE pE~ATx` liCTiHgA.
C*    KAPTA *NO LIST  ( *FULL LIST ),
C*    BCTPETiB{AqCq B l`bOM MECTE TEKCTA
C*    pPOgPAMMy, ByKl`~AET (BKl`~AET) pE~ATx
C*    liCTiHgA.  CTPOKi, COdEPvA}iE O{ibKi,
C*    A TAKvE diAgHOCTiKA, pE~ATA`TCq BCEgdA.
C*
      COMMON /CARD/ K(14)  /LEXEM/ LEX(14)
      COMMON /ACC/ ACC(10) /MEMOR/ KEY(2),LW4
      COMMON /ASSEM/ NSTEP,MER,NER,LF,NPC,
     *               NAMERR,KTYP,LABEL,KOP
      COMMON /ENTRY/ KENT,MENT(2,100)
      COMMON /REKV/ NAME,ITYP,KL,KR,IVAL
