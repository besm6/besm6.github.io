     *            'EQU','BLOCK','VALUE' /
C------------------------------------
      NPC=0 $ KTYP=2 $ IST=1 $ GOTO 9
   2  CALL MICPRI $ NPC=NPC+NDPC
   9  CALL MICREA $ NDPC=0
      MER=1 $ IT=2*KTYP+IST-2
      GOTO (2,2,2,2,3,6,4,4,999,999), IT
C---    dOvidAEMCq zAgOlOBKA
   4  MER=15 $ GOTO 2
   3  MER=2 $ IF(KOP.NE.INSTR(1)) GOTO 2
      IF(LABEL.EQ.' ') GOTO 2
