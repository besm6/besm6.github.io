      FUNCTION MICCAT(STR)
C*
C***   pE~ATx KATAlOgA APXiBA KPOCC-CiCTEMy
C*        *CALL MICCAT [ :ROM ]
C*
      COMMON /PROTO/ PROTO(30)
      COMMON /REKV/ NAME,ITYP,KL,KR,IVAL
      COMMON /MEMOR/ KEY(2),LW4(2),LROM,LFREE(2),LTAB,IROM
      COMMON /ASSEM/ NSTEP(4),NPC(4),KOP
      COMMON /BINARY/ BIN(4)
      DIMENSION MCN(5),LEX(14),STR(4),ACC(4),
     *          MC(5,1000),NC(2,2000),LC(99)
      EQUIVALENCE (MC,NC),(MC,LC(2))
      DATA MC/5000(' ')/, K/0/, LEX/14(' ')/
      DATA MCN/' VAR*  GLOB  FLD   PROG  ENTR'/
C-------------------------------------------
      LW4 = LROM.AND.377B.OR.0
      CALL SCANER(STR(2),LEX,0,ITYP,KOD,IP)
      CALL SCANER(0,LEX,3,ITYP,KOD,IP)
      CALL DIALOP(1) $ IREZ='SCAN'
      CALL VMEMOR(300000B) $ MICCAT=0
      CALL VMREAD(KEY,0,9) $ MROM=LEX
      NSTEP=3 $ IF(KEY.EQ.'MICROB') GOTO 9
      CALL DIALOG('APXiB pPOgPAMM pYCT.',-24,-1)
      GOTO 999
  9   CALL MICSEQ(IREZ) $ IF(IREZ.NE.0) GOTO 10
      K=K+1 $ MC(1,K)=NAME $ MC(2,K)=MCN(ITYP)
      GOTO (1,2,3,4,5), ITYP
  2   IF(KR.NE.' *ROM') GOTO 1
      NPC=IVAL $ CALL MICHEX(MC(3,K))
      MC(4,K)=' ' $ MC(2,K)=KR $ GOTO 9
  1   ITEMP=KR.SHIFT.47 $ LEX=' '
      IF(ITEMP.NE.0B) CALL PUTG(LEX,KR,0,6,0,IP)
      IF(ITEMP.NE.0B) KR=LEX
  5   CALL PUTG(MC(4,K),IVAL,1,6,0,IP)
      IF(KR.EQ.0B) KR=' '
      MC(3,K)=KR $ GOTO 9
  4   KL=IVAL
  3   CALL PUTG(MC(3,K),KL,0,6,0,IP)
      CALL PUTG(MC(4,K),KR,1,6,0,IP) $ GOTO 9
C---
 10   CALL SHSORT(MC,K,5,1) $ N=1
      MC(1,K+1)='CBObOd' $ MC(2,K+1)='HO - '
      CALL PUTG(MC(3,K+1),LFREE,1,6,0,IP)
      MC(1,K+2)='BCEgO ' $ MC(2,K+2)='iMEH :'
      CALL PUTG(MC(3,K+2),K,1,6,0,IP) $ JLC=1
      MC(1,K+3)='PAzMEP' $ MC(2,K+3)=' TAbl:'
      CALL PUTG(MC(3,K+3),LTAB,1,6,0,IP) $ K=K+3
      CALL DIALOG(' KATAlOg APXiBA:',-18,-1)
 11   LC(JLC)=' ' $ CALL DIALOG(LC(JLC),-126,-1)
      JLC=JLC+20 $ N=N+4 $ IF(N.LE.K) GOTO 11
      IF(MROM.NE.'ROM') GOTO 999
C---
      NSTEP=0 $ KOP='CONST'
      N=0 $ DO 12 I=1,K
      IF(MC(2,I).NE.' *ROM') GOTO 12
      N=N+1 $ NC(2,N)=MC(1,I) $ ITEMP=MC(3,I)
      NC(1,N)=0 $ DO 13 J=1,4
      LET=ITEMP.SHIFT.40.OR.0 $ ITEMP=ITEMP.SHIFT.-8
      LET=LET-48 $ IF(LET.GT.9) LET=LET-7
  13  NC(1,N)=NC(1,N).SHIFT.-4.OR.LET
  12  CONTINUE
      CALL SHSORT(NC,N,2,2)
      CALL DIALOP(2) $ CALL MBWDEF(ACC,96)
      CALL DIALOG('  p z Y :',-10,-1)
      DO 14 I=1,N $ NPC=NC(1,I)
      LEX(1)=NC(2,I) $ LEX(2)='     :'
      CALL VMREAD(BIN,IROM+2*NPC,2)
      CALL MICHE1(LEX(3))
  14  CALL DIALOG(LEX,-60,-1)
      CALL DIALOP(1)
 999  RETURN
      END
      SUBROUTINE MICSEQ(IREZ)
C*
C***         PAbOTA C TAblicEj iMEH
C*     KAvdyj ObxEKT zAHiMAET B TAblicE 5 ClOB.
C*
C*  1.NAME = iMq pOlq, pEPEMEHHOj (lOKAlxHOj
C*        ili glObAlxHOj), pPOgPAMMy (BXOdA).
C*
C*  2.ITYP = Tip iMEHi :
C*
C*    2.1. ITYP=1(2) - lOKAlxHAq (glObAlxHAq)
C*         pEPEMEHHAq. OCTAlxHyE ClOBA :
C*      3: C~ET~iK pOBTOPHyX OpiCAHij
C*      4: iMq BHE{HEgO ObxEKTA ili
C*         OTHOCiTElxHyj AdPEC+1  ili 0.
C*      5: AbCOl`THAq KOMpOHEHTA zHA~EHiq
C*
C*    2.3. ITYP=3 - pOlE.  OCTAlxHyE ClOBA:
C*      3: lEBAq gPAHicA pOlq
C*      4: pPABAq gPAHicA pOlq
C*      5: zHA~EHiE pOlq pO YMOl~AHi`
C*
C*    2.4. ITYP=4 - pOdpPOgPAMMA :
C*      3: dliHA pOdpPOgPAMMy B pAMqTi
C*      4: dliHA pOdpPOgPAMMy B bibliOTEKE
C*      5: HA~AlxHyj AdPEC B bibliOTEKE
C*
C*    2.5. ITYP=5 - BXOd (ENTRY) :
C*      3: pYCTO
C*      4: iMq gOlOBHOj pPOgPAMMy
C*      5: OTHOCiTElxHyj AdPEC BXOdA
C*
C*     HAbOP |lEMEHTAPHyX OpEPAcij C TAblicEj:
C*      MICGID - pOiCK i izBlE~EHiE
C*      MICPID - zAHECEHiE iMEHi B TAblicY
C*      MICCLE - izxqTiE lOKAlxHyX iMEH
C*      MICSEQ - pOClEdOBATElxHOE CKAHiPOBAHiE
C*      IREZ = 0, ECli iMq HAjdEHO B TAblicE.
C*
      COMMON/REKV/ NAME,ITYP(4)
      COMMON/MEMOR/ KEYP,LW(5),ITAB,LTAB
      DIMENSION KVANT(5),MAPK(5)
      DATA NORM/0/, MABS/1/, MDEL/77777777B/
      DATA MAPK /200B,200B,100000B,0B,0B/
C----------------------------------
      IF(IREZ.EQ.'SCAN') IA2=0
      IREZ=MABS $ ITYP=1
 100  IF(IA2.GE.LTAB) RETURN
      CALL VMREAD(KVANT,ITAB+5*IA2,5)
      IA2=IA2+1
      IF(KVANT.EQ.0B.OR.KVANT.EQ.MDEL) GOTO 100
      IREZ=NORM $ GOTO 30
C---
      ENTRY MICGID
      JOB=1 $ GOTO 1
      ENTRY MICPID
      JOB=2
C---   "PAzMAzyBAHiE" TipA pO pOl` iMEHi
  1   I=NAME.SHIFT.24 $IF(I.NE.0B) I=MAPK(ITYP)
      NAM=NAME.OR.I
      KEY=LEXOR(NAM,LSHIFT(NAM,25))
      KEY=KEY.AND.177777777B.OR.0
C---   pOlY~EHiE HA~.AdPECA = 0,1,...,LTAB-1
      IA=KEY/LTAB
      IA=KEY-IA*LTAB $ IA1=IA
C---   XE{iPOBAHiE
  2   CALL VMREAD(KVANT,ITAB+5*IA,5)
      IREZ=MABS
      IF(KVANT.EQ.0B)  GOTO (10,20), JOB
      IF(KVANT.EQ.MDEL) GOTO (3,20), JOB
      IREZ=NORM
      IF(KVANT.EQ.NAM) GOTO (30,20), JOB
  3   IA=IA+1 $ IF(IA.EQ.IA1) STOP
      IF(IA.GE.LTAB) IA=0
      GOTO 2
C---   By{li HA pYCTOj KBAHT ili HA{li iMq
 20   KVANT = NAM  $ DO 21 I=1,4
 21   KVANT(I+1)=ITYP(I)
      CALL VMWRIT(KVANT,ITAB+5*IA,5)
      RETURN
C---   KBAHT COdEPviT iCKOMOE iMq
 30   DO 31 I=1,4
 31   ITYP(I)=KVANT(I+1)
      NAME=KVANT $ I=NAME.SHIFT.24
      IF(I.NE.0B) NAME=KVANT.AND.3767 7577 3767 7577B
 10   RETURN
C---
      ENTRY MICCLE
      IA=ITAB $ IREZ=NORM
      DO 90 I=1,LTAB $ CALL VMREAD(KVANT,IA,5)
      J=KVANT(2).OR.0 $ IF(J.LT.3) CALL WRVMEM(0,IA+2)
      IF(J.LE.ITYP) CALL VMSET(0B,IA,5)
 90   IA=IA+5
      RETURN
      END
      SUBROUTINE MICVAL(JOB)
C*
C***         By~iClEHiE ByPAvEHiq
C*    JOB - dOpYCTiMyj Tip ByPAvEHiq:
C*      1 : AbCOl`THOE
C*      2 : OTHOCiTElxHOE, T.E. dOpYCKA`TCq
C*          CCylKi HA OTHOCiTElxHyE AdPECA
C*          ili BHE{HiE ObxEKTy.
C*
      COMMON /LEXEM/ LEX(14),IP
      COMMON /REKV/ NAME,ITYP,NDEF,NREL,IVAL
      COMMON /ASSEM/ NSTEP,MER(3),NPC,NAMERR
      COMMON /LETTER/ LET,KTP(3),KPLUS,
     *                KMINUS(4),KHEX
      COMMON /SEMA/ JF(203),MRF(100),MLS(2)
C--------------------------------
      MF=0 $ GOTO 33
C---    BXOd dlq TPAHClqcii OpiCAHiq pOlEj
      ENTRY MICFIL
      MF=1
  33  IVABS=0 $ IVREL=0 $ IVEXT=0
      MSIG=0 $ NAMC=NAME $ ITYPC=ITYP
   1  CALL SCANER(0,LEX,2,IT,LET,IP)
      MER=7 $ IT=IT+1 $ NAMERR=6H
      GOTO (9,2,3,9,10,22), IT
C---   {ECTHAdcATiPi~HOE :  $<~iClO>
  22  LEX=0 $ IF(LET.NE.KHEX) GOTO 9
  23  CALL SCANER(0,NHEX,13B,IT,LET,IP)
      IF(IT.NE.1.AND.IT.NE.2) GOTO 2
      NHEX=LET-48 $ IF(IT.EQ.2) NHEX=NHEX-7
      IF(NHEX.LT.0.OR.NHEX.GT.15) GOTO 9
      LEX=LEX.SHIFT.-4.OR.NHEX $ GOTO 23
C---   cElOE ili BOCxMEPi~HOE
   2  LEX=LEX.OR.0
   3  IF(MSIG.NE.0) LEX=-LEX
      IVABS=IVABS+LEX $ GOTO 4
C---    pOlY~ili idEHTifiKATOP
  10  CONTINUE
      IF(LEX.NE.'*') GOTO 19
      IF(JOB.EQ.1) GOTO 9
      NREL=NPC+1 $ IVAL=0 $ GOTO 12
  19  IF(JF.EQ.0) GOTO 11
      J2=MRF(JF).SHIFT.12.AND.7777B.OR.0
      J1=MRF(JF).AND.7777B.OR.0
      IF(J2.EQ.0) GOTO 11
      DO 18 I=1,J2
      IF(MLS(J1).EQ.LEX) GOTO 11
  18  J1=J1+1
      J2=MRF(JF).SHIFT.24.AND.7777B.OR.0
      CALL PUTH(NAMERR,'FL',0,2,0,J1)
      CALL PUTG(0,J2,0,4,0,J1)
      MER=14 $ GOTO 9
C---    izBlE~EHiE ATPibYTOB iz TAblicy
  11  NAME=LEX $ ITYP=2 $ CALL MICGID(IREZ)
      MER=5 $ IF(IREZ.EQ.0) GOTO 12
      IVABS=LEX $ NAMERR=NAME
      IF(MF.EQ.0) GOTO 9
      IF(IVREL+IVEXT.EQ.0) MER=0
      GOTO 9
  12  LEX=IVAL $ MER=6 $ IF(NREL.EQ.0) GOTO 3
      IF(MSIG.NE.0) LEX=-LEX
      IVABS=IVABS+LEX $ MER=12
      IT=NREL.SHIFT.42 $ IF(IT.EQ.64B) GOTO 13
      IF(MSIG.NE.0.OR.IVEXT.NE.0) GOTO 9
      IVEXT=NREL $ GOTO 4
  13  IF(IVREL.NE.0) GOTO 14
      IF(MSIG.NE.0) GOTO 9
      IVREL=NREL $ GOTO 4
  14  IF(MSIG.EQ.0) GOTO 9
      IVABS=IVABS+IVREL-NREL-1 $ IVREL=0
C---    Y~ET zHAKA ClEdY`}EgO TEPMA
   4  MSIG=0 $ IF(LET.EQ.KPLUS ) GOTO 1
      MSIG=1 $ IF(LET.EQ.KMINUS) GOTO 1
C---    KOHEc ByPAvEHiq. AHAliz TipA
 100  CONTINUE
      MER=0 $ NREL=IVEXT
      IF(NREL.EQ.0) GOTO 101
      IF(JOB.EQ.1.OR.IVREL.NE.0) MER=12
      GOTO 9
 101  NREL=IVREL
      IF(NREL.NE.0.AND.JOB.EQ.1) MER=12
C---
   9  NAME=NAMC $ ITYP=ITYPC
      NDEF=0 $ IVAL=IVABS
      RETURN
      END
      FUNCTION MICROB(STR)
C*
C***   gOlOBHAq pPOgPAMMA MiKPOACCEMblEPA
C*
C*    *CALL MICROB [:<YK.Ml>,<CpiCOK iMEH>]
C*    *CALL MICINI [:<YK.Ml>,<CpiCOK iMEH>]
C*
C*    MICINI - BXOd dlq HACTPOjKi ACCEMblEPA
C*    MICROB - BXOd dlq TPAHClqcii pPOgPAMM.
C*
C*    HACTPOjKA CBOdiTCq K TPAHClqcii
C*    "pPOgPAMMy" C iMEHEM DEFINE, OpiCyBA`-
C*    }Ej fOPMAT MiKPOKOMAHdy, A TAKvE glO-
C*    bAlxHyE pEPEMEHHyE BCEX MiKPOpPOgPAMM.
C*
C*    <YK.Ml> zAdAET fAjl C TEKCTOM, pOdlEvA-
C*    }iM TPAHClqcii. pPizHAK KOHcA TEKCTA -
C*    CTPOKA "*END".  pPi HEYKAzAHii fAjlA
C*    TEKCT ~iTAETCq C pEPfOKAPT.
C*
C*    pEPEd ObPA}EHiEM K ACCEMblEPY ClEdYET
C*    zAKAzATx pOd HOMEPOM 30 fAjl dlq APXiBA.
C*    APXiB COdEPviT :
C*     1. OpPEdElEHiq glOb.pEPEMEHHyX i pOlEj
C*     2. bibliOTEKA MOdYlEj zAgPYzKi
C*     3. PAbO~iE OblACTi pAMqTi TPAHClqTOPA.
C*
      DIMENSION STR(2)
      COMMON /PROTO/ PROTO(30) /NMLST/ NML(50)
      COMMON /ASSEM/ NSTEP(2),NER(3),NAMERR(4)
      COMMON /CARD/ K(200) /LEXEM/ LEX(14),IPOZ
      COMMON /MEMOR/ KEY,LW,LW4,LB,LROM,LFREE,
     *               ITAB,LTAB,IROM,JSTOP
      COMMON /REKV/ NAME,ITYP,KL,KR,IVAL
      COMMON /ACC/ ACC(10)  /BINARY/ BIN(10)
      COMMON /FIELD/ NFIL,LFIL(4,100),NVF(100)
      COMMON /ENTRY/ KENT,MENT(2,100)
      COMMON /EXTERN/ KEXT,MEXT(400)
      COMMON /SEMA/ SEMA(1803)
      COMMON /LETTER/ LET,KTP,KSEM,KCOMMA,KPL,
     *     KMIN,KSTAR,KSL,KE,KHEX,KCONT
      DATA NFIL/0/,LROM/0/,MAXZON/127/
      DATA ITAB/20/, LTAB/1501/, K/200(' ')/
      DATA KTP/58/,KSEM/59/,KCOMMA/44/,KE/61/,
     *     KPL/43/,KMIN/45/,KSTAR/42/,KSL/47/,
     *     KHEX/36/,KCONT/62/
C----------------------------
      JOB=1 $ GOTO 1
      ENTRY MICINI
      JOB=2 $ MICROB=0
C---    BybOPKA <YK.Ml> i CpiCKA iMEH
  1   MT='CARD' $ NML(1)=0 $ NML(2)=1
      CALL SCANER(STR(2),LEX,0,IT,KOD,IPOZ)
      IF(KOD.NE.KTP) GOTO 2
      CALL SCANER(0,LEX,7,IT,KOD,IPOZ)
      IF(IT.EQ.4) GOTO 12
      MT=LEX $ NZ=MT.AND.777B
      NU1=MT.SHIFT.9 $ NU=MT.SHIFT.12
      J=NU.SHIFT.3 $ IF(J.EQ.0B) NU=NU1
      MT=(NU.SHIFT.-12).OR.NZ
      IF(MT.EQ.1B) MT=03 0000B
      IF(MT.EQ.2B) MT=05 0000B
  11  IF(KOD.NE.KCOMMA) GOTO 2
      CALL SCANER(0,LEX,3,IT,KOD,IPOZ)
  12  NML(2)=2 $ NML(1)=NML(1)+1
      NML(NML+2)=LEX $ GOTO 11
C---    iHiciAlizAciq ~iTAlKi i APXiBA
  2   CALL RCINIT(MT)
      CALL VMEMOR(30 0000B)
      CALL VMREAD(KEY,0,6)
      NER=0 $ NFIL=0 $ NAMERR=6H
      IF(JOB.EQ.2.OR.KEY.NE.'MICROB')CALL MICGEN
      CALL VMREAD(KEY,0,9)
      IF(LFREE.GT.1024*MAXZON) NER=-1
      IF(NER.EQ.-1) CALL DIALOG('APXiB pOlOH',-12,-1)
      IF(NER.NE.0) STOP
      LB=LW4/48 $ IF(48*LB.NE.LW4) LB=LB+1
C---    CObCTBEHHO TPAHClqciq i zAKPyTiE APXiBA
      IPOZ=-1 $ JSTOP=0 $ CALL MICASS
      CALL VMWRIT(KEY,0,ITAB) $ CALL VMCLOS
      IF(JSTOP.EQ.0) RETURN
      CALL DIALOG('byli O{ibKi pPi TPAHClqcii.',-27,-1)
      STOP
      END
      FUNCTION MICSET(KOD,KL,KR)
C*
C***   zAgPYzKA pOlq [KL:KR] MiKPOClOBA
C*     COdEPviMyM MlAd{iX PAzPqdOB KOD.
C*     ECli iHfOPMAciq "HE BlEzAET" B
C*     pOlE, PEzYlxTAT HE PABEH 0.
C*
      COMMON /MEMOR/ KEY(2),LW4
C---
      MICSET=0
      IF(KOD.EQ.'/C/'.OR.KOD.EQ.'/A/') RETURN
      KK = KOD.AND.7777 7777B
      N = LW4+1-KR
      LOOP = 1+KL-KR
      DO 1 I=1,LOOP
      KB=KK.AND.1B
      IF(KB.EQ.0B) CALL MBWCLE(N)
      IF(KB.EQ.1B) CALL MBWPUT(N)
      KK=KK.SHIFT.1
   1  N=N-1
      MICSET=KK.OR.0
      RETURN
C
C---
C
      ENTRY MICFRG
C*
C***   ObPATHAq pPOcEdYPA: BydA~A
C*     COdEPviMOgO pOlq [KL:KR] .
C*     pOPqdOK PEzYlxTATA COBpAdAET
C*     C pOPqdKOM ClOBA  KOD.
C*
      KK=0B
      LOOP=1+KL-KR
      N=LW4+1-KL
      DO 2 I=1,LOOP
      KK=KK.SHIFT.-1
      IF(MBWCHK(N).NE.0) KK=KK.OR.1B
   2  N=N+1
      MICSET=KOD.AND.7740 0000 0000 0000B.OR.KK
      RETURN
      END
      SUBROUTINE MICAN1
C*
C***   pEPBi~HOE PACpOzHABAHiE KAPTy.
C*    fOPMiPY`TCq pEPEMEHHyE: KTYP,LABEL,KOP
C*
C*        KTYP - Tip KAPTy :
C*    1 - YpPABlEHiE TPAHClqciEj
C*    2 - KOMMEHTAPij
C*    3 - iHCTPYKciq ACCEMblEPA
C*    4 - HEOpOzHAHHAq KAPTA
C*    5 - KOHEc fAjlA ~TEHiq
C*
C*        LABEL = <METKA> ili 6H
C*         KOP  = <KOd OpEPAcii> ili 6H
C*
      COMMON /CARD/ K(14)
      COMMON /LEXEM/ LEX(14),IP
      COMMON /LETTER/ LET,KTP,KSEM(4),KSTAR
      COMMON /ASSEM/ NSTEP,MER(5),KTYP,LABEL,KOP
      COMMON /ENTRY/ KENT,MENT(2,100)
C-------------------------
      LABEL=6H
      KOP=6H
      KTYP=4
      CALL SCANER(K,LEX,2,IT,LET,IP)
      IF( IT-4 ) 10,4,1
C---     idEHTifiKATOP
  4   LABEL=LEX
      IF(LET.EQ.KTP) GOTO 2
      LABEL=6H
      KTYP=1
      IF(LEX.EQ.'*NO'.OR.LEX.EQ.'*FULL') GOTO 10
      KTYP=5
      IF(IGETS(LEX,0).EQ.KSTAR) GOTO 10
      GOTO 5
C---     pYCTOj fPAgMEHT
  1   IF(LET.EQ.KSEM) KTYP=2
      IF(LET.NE.KTP) GOTO 10
C---     BybOPKA  K O p
  2   CALL SCANER(0,LEX,0,IT,LET,IP)
      IF(IT.NE.4) GOTO 10
  5   KOP=LEX
      KTYP=3
  10  RETURN
C*
C*
C***  BybOPKA {ECTHAdcATiPi~HOj XAPAKTEPiCTiKi
C*    fOPMiPYETCq MENT. ECli HET O{ibKi, MER=1
C*
      ENTRY MICHCH
      NHEX=0 $ ITEMP=0B
      IF(LET.LT.32.OR.LET.EQ.KSEM) GOTO 20
  21  CALL SCANER(0,NHEX,13B,IT,LET,IP)
      IF(LET.LT.32.OR.LET.EQ.KSEM) GOTO 20
      NHEX=LET-48 $ IF(NHEX.GT.9) NHEX=NHEX-7
      IF(NHEX.LT.0.OR.NHEX.GT.15) RETURN
      ITEMP=ITEMP.SHIFT.-4.OR.NHEX $ GOTO 21
  20  MER=1 $ MENT(2,KENT)=ITEMP.SHIFT.-24
      RETURN
      END
      SUBROUTINE MICGEN
C*
C***   TPAHClqciq glObAlxHOgO KOHTEKCTA.
C*      (pPOizBOdiTCq zA OdiH pPOXOd).
C*
C*    glObAlxHyj KOHTEKCT OpiCyBAETCq
C*    pCEBdOpPOgPAMMOj C iMEHEM  DEFINE:
C*       DEFINE:PROG <~iClO>,<~iClO1>;
C*    zdECx <~iClO> zAdAET PAzPqdHOCTx
C*    MiKPOClOBA  N.  biTy B MiKPOClOBE
C*    HYMEPY`TCq CpPABA HAlEBO: N,N-1,...,2,1
C*    <~iClO1> - PAzPqdHOCTx pzY KOHCTAHT
C*    pPi YMOl~AHii = 64.
C*
C*    KOHEc pPOgPAMMy - KOHCTPYKciq   "END;"
C*    CiMBOl ";" - pPizHAK KOHcA l`bOj CTPOKi.
C*    pOClE HEgO MOvET ClEdOBATx pPOizBOlxHyj
C*    KOMMEHTAPij. BHYTPi pPOgPAMMy  DEFINE
C*    dOpYCTiMy  ClEdY`}iE KOHCTPYKcii :
C*
C*     1. OpPEdElEHiE glObAlxHOj pEPEMEHHOj.
C*      <METKA>:EQU <ByPAvEHiE>;
C*      B <ByPAvEHiE> MOgYT BXOdiTx CO zHAKOM
C*      "+" ili "-" ~iClA, A TAKvE pEPEMEHHyE,
C*      zHA~EHiq KOTOPyX K |TOMY MOMEHTY YvE
C*      OpPEdElEHy.
C*
C*     2. gPYppOBOE OpPEdElEHiE iMEH.
C*      A:BLOCK B,C(5),D;      - |KBiBAlEHTHO
C*      B:EQU A;  C:EQU B+1;  D:EQU C+5;
C*
C*     3. OpPEdElEHiE pOlq.
C*      <METKA>:FIELD <N1>,<N2>[,<VALUE>];
C*      zdECx MAX(N1,N2) zAdAET lEBY`,  A
C*      MIN(N1,N2) - pPABY` gPAHicy pOlq.
C*      HEObqzATElxHOE <VALUE> (pPi HEYKA-
C*      zAHii pOlAgAETCq PABHyM HYl`)
C*      zAdAET KOd, KOTOPyM zApOlHqETCq pOlE
C*      pPi HEYKAzAHii EgO B MiKPOKOMAHdE.
C*
C*     4. OpPEdElEHiE KOHCTAHTy iz  p z Y.
C*      <METKA>:CONST <{ECTH.KOHCTAHTA>;
C*      lEByE HYli B KOHCTAHTAX MOvHO OpYCKATx.
C*      KOHCTAHTy AdPECY`TCq B  p z Y  HA~iHAq
C*      C AdPECA 0 B pOPqdKE iX zABEdEHiq.
C*
      COMMON /CARD/ K(14)  /LEXEM/ LEX(14),IP
      COMMON /LETTER/ LET,KTP,KSEM,KCOMMA(7),KCONT
      COMMON /MEMOR/ KEY,LW,LW4,LBIN,LROM,
     *               LFREE,ITAB,LTAB,IROM
      COMMON /REKV/ NAME,ITYP,KL,KR,IVAL
      COMMON /ACC/ ACC(10)
      COMMON /BINARY/ BIN(10)
      COMMON /FIELD/ NFIL,LFIL(4,100)
      COMMON /ASSEM/ NS,MER,NER,LFAT,NPC,
     *        NAMERR,KTYP,LABEL,KOP
      INTEGER ACC
C----------------------------------
C
C       OCHOBHyE pAPAMETPy TPAHClqTOPA :
C       ================================
C
C   KEY   - KOd HAli~iq APXiBA
C   LW    - PAzPqdHOCTx MiKPOClOBA
C   LW4   -  ---"---, OKPYglEHHAq dO 4
C   LBIN  - dliHA MiKPOClOBA B ClOBAX b-6
C   LROM  - PAzPqdHOCTx pzY (1:9) i
C           KOli~ECTBO KOHCTAHT (10:24)
C   LFREE - CBObOdHOE MECTO B APXiBE
C   ITAB  - HA~AlO TAblicy iMEH B APXiBE
C   LTAB  - PAzMEP TAblicy iMEH
C   IROM  - HA~AlO CpiCKA  pzY-KOHCTAHT
C
      KEY='MICROB'
      LFREE=ITAB+5*LTAB+1
      CALL VMSET(0B,ITAB,5*LTAB)
      NFIL=0 $ LW=0 $ NS=2 $ NAMERR=6H
      MER=2 $ LFAT=1 $ NER=0 $ NPC=-1
      CALL READER(K) $ KROM=0 $ LROM=64
      CALL MICAN1 $ IROM=LFREE
      IF(LABEL.NE.'DEFINE') GOTO 1
      IF(KOP.NE.'PROG') GOTO 1
      MER=3
      CALL SCANER(0,LW,0,IT,LET,IP)
      IF(IT.NE.2.OR.LW.LT.10) GOTO 1
      IF(LET.EQ.KCOMMA) CALL SCANER(0,LROM,0,IT,LET,IP)
      IF(IT.NE.2.OR.LROM.GT.96) GOTO 1
      N1=LROM/4 $ IF(LROM.NE.4*N1) GOTO 1
      MER=1 $ LFAT=0
      LW4=LW/4
      IF(4*LW4.NE.LW) LW4=LW4+1
      LW4=4*LW4
C---     ciKl pOCTPO~HOj TPAHClqcii
   1  CALL MICPRI
      CALL READER(K)
      IF(IGETS(K,0).EQ.KCONT)
     *            CALL IPUTS(KSEM,K,0)
      CALL MICAN1
      CALL MBWDEF(ACC,LW4) $ CALL MBWSTA(BIN)
      MER=1 $ IF(KTYP.LT.3) GOTO 1
      MER=15 $ IF(KTYP.NE.3) GOTO 1
      MER=1 $ IF(KOP.EQ.'END') GOTO 999
      MER=7 $ IF(LABEL.EQ.' ') GOTO 1
C---     pOlY~EHA METKA
      NAME = LABEL $ ITYP=2
      IF(KOP.EQ.'EQU') GOTO 10
      IF(KOP.EQ.'BLOCK') GOTO 100
      IF(KOP.EQ.'CONST') GOTO 200
      MER=15 $ IF(KOP.NE.'FIELD') GOTO 1
C---     |TO OpiCAHiE pOlq
      ITYP=3 $ MER=9
      CALL SCANER(0,N1,0,IT,KOD,IP)
      IF(IT.NE.2.OR.N1.LT.1.OR.N1.GT.LW) GOTO 1
      CALL SCANER(0,N2,0,IT,KOD,IP)
      IF(IT.NE.2.OR.N2.LT.1.OR.N2.GT.LW) GOTO 1
      IF(N1.GE.N2) GOTO 3
      J=N1 $ N1=N2 $ N2=J
   3  IVAL=0B $ IF(KOD.NE.KCOMMA) GOTO 4
      CALL MICFIL(1)
      IF(MER.GT.1) GOTO 1
   4  NF1=NFIL $ NFIL=NFIL+1
      LFIL(1,NFIL)=NAME
      LFIL(2,NFIL)=N1
      LFIL(3,NFIL)=N2
      LFIL(4,NFIL)=IVAL
      KL=N1 $ KR=N2 $ MER=11
      IF(NF1.EQ.0) GOTO 20
      DO 5 I=1,NF1
      NAMERR=LFIL(1,I)
      KL1=LFIL(2,I) $ KR1=LFIL(3,I)
      IF(KR1.GE.KL) GOTO 5
      IF(KR1.GT.KR) GOTO 1
      IF(KL1.LE.KR) GOTO 5
      IF(KL1.LT.KL) GOTO 1
   5  CONTINUE
      GOTO 20
C---
 100  CALL MICBLO(2) $ GOTO 1
C
C---    KOHCTAHTA iz  p z Y
 200  CALL MBWDEF(ACC,LROM)
      ACC(2)=0B $ ACC(3)=0B $ MER=7
 201  CALL SCANER(0,LEX,13B,IT,LET,IP)
      IF(LET.EQ.KSEM) GOTO 203
      IF(IT.EQ.1) GOTO 202
      IF(IT.NE.2) GOTO 1
      IF(LET.GT.70) GOTO 1
      LET=LET-7
 202  LET=LET-48
      J=ACC.SHIFT.44 $ IF(J.NE.0B) GOTO 1
      ACC(1)=ACC(1).SHIFT.-4.OR.ACC(2).SHIFT.44
      ACC(2)=ACC(2).SHIFT.-4
      N2=LW4 $ LW4=LROM
      J=MICSET(LET,4,1)
      LW4=N2 $ GOTO 201
 203  KROM=KROM+1 $ J=IROM+2*KROM-2
      CALL VMWRIT(ACC,J,2)
      CALL MBWDEF(ACC,LW4)
      KL=0 $ KR=' *ROM ' $ IVAL=KROM-1
      CALL VMREAD(ACC,J,2)
      GOTO 11
C---    OpiCAHiE glObAlxHOj pEPEMEHHOj
  10  CALL MICVAL(1)
      IF(MER.GT.1) GOTO 1
      KL=0 $ KR=0
  11  N2=1 $ N1=LW $ IF(N1.GT.24) N1=24
C---    PEgiCTPAciq B TAblicE
  20  CALL MICPID(IREZ)
      NAMERR=NAME $ MER=6
      IF(IREZ.EQ.0) GOTO 1
      MER=10
      IF(MICSET(IVAL,N1,N2).NE.0) GOTO 1
      CALL MBWSTA(BIN)
      NAMERR=' ' $ MER=0 $ GOTO 1
 999  NFIL=0 $ CALL MICCHF
      CALL MICPRI
      LROM=KROM.SHIFT.-9.OR.LROM
      LFREE=ITAB+5*LTAB+2*KROM+1
      IF(NER.EQ.0) CALL VMWRIT(KEY,0,ITAB)
      RETURN
      END
      SUBROUTINE MICCHF
C*
C***   iTOgOBAq pPOBEPKA OpiCAHij pOlEj
C*     COPTiPOBKA pOlEj pO YbyBAHi` PAzMEPOB
C*
C*     CPEdi BCEX pOlEj MiKPOKOMAHdy
C*     dOlvHO byTx POBHO OdHO pOlE
C*     KOdA OpEPAcii i POBHO OdHO
C*     AdPECHOE pOlE.
C*
C*     dlq cElEj MAPKiPOBKi pOlEj ClYviT
C*     CpEciAlxHOE zHA~EHiE ATPibYTA <VALUE>
C*     (CM. MICGEN).
C*     <VALUE> = /C/   - pOlE KOdA OpEPAcii,
C*     <VALUE> = /A/   - AdPECHOE pOlE
C*
C*     pOlE KOp pOME}AETCq HA pEPBOE MECTO
C*     B CpiCKE pOlEj LFIL, AdPECHOE pOlE -
C*     - HA BTOPOE.
C*
      COMMON /FIELD/ NFIL,LFIL(4,100),NVF(100)
      COMMON /ASSEM/ NSTEP,MER,NER(3),NAMERR
      COMMON /REKV/ NAME,ITYP,KL,KR,IVAL
      COMMON /LEXEM/ LEX(14),IPOZ
      COMMON /ENTRY/ KENT,MENT
      COMMON /ERTEX/ ISP(20),MD(5,20)
      DIMENSION KAC(2),MAPK(2)
      DATA MAPK/ '/C/   /A/' /
C-------------------------------------
      IPOZ=-1 $ IF(NER.NE.0) GOTO 9
      IF(NFIL.NE.0) GOTO 6
      IREZ='SCAN' $ KAC(1)=0 $ KAC(2)=0
  10  CALL MICSEQ(IREZ)
      IF(IREZ.NE.0) GOTO 11
      IF(ITYP.NE.3) GOTO 10
      NFIL=NFIL+1 $ IC=0
      DO 2 J=1,2
      IF(IVAL.NE.MAPK(J)) GOTO 2
      KAC(J)=KAC(J)+1
      IC=4096/J
   2  CONTINUE
      LFIL(2,NFIL)=NAME
      LFIL(1,NFIL)=KL-KR+IC
      LFIL(3,NFIL)=KR
      LFIL(4,NFIL)=IVAL $ GOTO 10
  11  NAMERR=' ' $ MER=8
      IF(NFIL.EQ.0) RETURN
      MER=13
      NAMERR=MAPK(1) $ IF(KAC(1).NE.1) RETURN
      NAMERR=MAPK(2) $ IF(KAC(2).NE.1) RETURN
      CALL SHSORT(LFIL,NFIL,4,3)
      DO 1 I=1,NFIL
      IC=LFIL(1,I).AND.777B.OR.0
      LFIL(1,I)=LFIL(2,I)
      LFIL(2,I)=LFIL(3,I)+IC
  1   NVF(I)=LFIL(4,I)
  6   MER=1 $ NAMERR=6H
      RETURN
  9   MER=20 $ CALL PUTG(NAMERR,NER,0,4,0,IP)
      MD(1,MER)=MENT $ RETURN
      END
      SUBROUTINE MICASS
C*
C***   pOO~EPEdHAq TPAHClqciq BCEX (ili pO
