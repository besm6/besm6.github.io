      LET=LOR(LET,LSHIFT(LCNT(LET),-7))
  13  CALL PTPUNS(LET)
      CALL PTPUNS(215B)
      CALL PTPUNS(012B)
      GOTO 11
  15  CALL PTPUNM( 5134 2717 6110 6412B, 6 )
      CALL PTPEND(240)
