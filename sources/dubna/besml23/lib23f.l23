      GO TO 25
28    CONTINUE
      DO 29K=1,NDIM
      IF(III(K).EQ.1)C(K)=-C(K)
29    CONTINUE
      N1=NN(KKKK)
      N2=NN(KKKK+1)
      NNN=((N2-N1)*(N2+N1+1))/2
      GO TO 31
30    KKKK=KKKK-1
      IF(KKKK.EQ.0)GO TO 33
      N1=NN(KKKK)
      N2=NN(KKKK+1)
      NNN=((N2-N1)*(N2+N1+1))/2
      BACKSPACE 1
      BACKSPACE 1
      READ(1)(A(J),J=1,LB)
31    CONTINUE
      IF(LLLL.EQ.N1)GO TO 30
      C(LLLL)=C(LLLL)/A(NNN)
      X1=C(LLLL)
