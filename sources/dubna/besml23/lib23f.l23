      IF(III(LLLL).EQ.1)A(LLL2)=-A(LLL2)
8     CONTINUE
      GO TO 7
11    CONTINUE
      LLL2=0
      LLLL=N1
12    LLL1=LLLL
      LLLL=LLLL+1
