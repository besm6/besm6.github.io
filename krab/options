krab_dir=`pwd`
std_disks=/usr/local/share/besm6
extra_disks=$krab_dir/disks
diskid_src=2086
diskid_mdl=2086
diskid_krab=2063
diskoff_src=0600
diskoff_mdl=1500
diskoff_krab=0000
disk_src=$extra_disks/$diskid_src
disk_mdl=$extra_disks/$diskid_mdl
disk_krab=$extra_disks/$diskid_krab
src=$krab_dir/src
logfile=work/$0.log

BESM6_PATH=$extra_disks:$std_disks
export BESM6_PATH
tf1=work/$$.1
tf2=work/$$.2

